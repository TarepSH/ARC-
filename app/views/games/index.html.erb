

<h1>Games</h1>

<table>
  <thead>
    <tr>
      <th>Team</th>
      <th>Round</th>
      <th>Team catagory</th>
      <th>Game point</th>
      <th>Game time</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @games.each do |game| %>
      <tr>
        <td><%= game.team_id %></td>
        <td><%= game.round_id %></td>
        <td><%= game.team_catagory %></td>
        <td><%= game.game_point %></td>
        <td><%= game.game_time %></td>


      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Game', new_team_game_path %>
